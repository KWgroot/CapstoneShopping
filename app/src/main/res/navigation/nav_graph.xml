<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/addGroceryFragment">
    <fragment
        android:id="@+id/addGroceryFragment"
        android:name="com.example.capstoneshopping.UI.AddGroceryFragment"
        android:label="fragment_fridgelist"
        tools:layout="@layout/fragment_fridgelist" >
        <action
            android:id="@+id/action_addGroceryFragment_to_groceryAdapter"
            app:destination="@id/groceryAdapter" />
    </fragment>
    <fragment
        android:id="@+id/groceryAdapter"
        android:name="com.example.capstoneshopping.UI.GroceryAdapter"
        android:label="fragment_item_add"
        tools:layout="@layout/fragment_item_add" >
        <action
            android:id="@+id/action_groceryAdapter_to_addGroceryFragment"
            app:destination="@id/addGroceryFragment" />
    </fragment>
</navigation>